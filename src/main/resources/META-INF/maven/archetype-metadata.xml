        <!--requiredProperty key="package"/-->
        <requiredProperty key="root-class">
           <defaultValue>CC</defaultValue>
        </requiredProperty>
        <!--requiredProperty key="servlet.name"/-->
    </requiredProperties>
  
    <fileSets>
